  "bugs": {
    "url": "https://github.com/SaltyQuetzals/multiplayer-game/issues"
  },
  "homepage": "https://github.com/SaltyQuetzals/multiplayer-game#readme",
  "dependencies": {
    "@microsoft/tsdoc": "^0.9.2",
    "chai": "^4.2.0",
    "express": "^4.16.4",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/socket.io": "^1.4.38",
